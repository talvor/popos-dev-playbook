# ls
alias ls='ls -F --color=auto'
alias la='ls -A'
alias ll='ls -l'
alias lla='ll -A'
alias llh='ll -h'
alias llha='ll -hA'

# grep
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'

# code
alias code="flatpak run com.visualstudio.code"

# toolbox
alias enter='distrobox enter'
